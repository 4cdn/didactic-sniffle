<html>
  <head>
    <title>foobar</title>
    <link type="text/css" href="/styles.css" rel="stylesheet">
    <style type="text/css">
      .mod_accepted tr:nth-child(2n+2) {
          background: none repeat scroll 0 0 #151515;
      }
      .mod_ignored tr:nth-child(2n+2) {
          background: none repeat scroll 0 0 #151515;
      }
      td, th {
        padding: 0px;
      }
      .key {
        text-align: right;
      }
      .date {
        padding-right: 10px;
      }
      .action {
        padding-left: 10px;
        text-align: center;
      }
      .postid {
        padding-left: 10px;
      }
      .reason {
        text-align: right;
        padding-left: 10px;
      }
      .actions {
        padding-left: 10px;
      }
      .data_center {
        text-align: center;
        padding-left: 10px;
      }
      .box {
        border: 1px dotted #AFAFAF;
      }
    </style>
  </head>
  <body class="mod">
    %%navigation%%
    <div class="box">
      <b>moderation accepted log</b>
      <table class="mod_accepted">
        <tr>
          <th>&nbsp;</th>
          <th>&nbsp;</th>
          <th>action</th>
          <th>target</th>
          <th>reason</th>
          <th>&nbsp;</th>
          <th>comment</th>
        </tr>
        %%mod_accepted%%
      </table>
    </div>
    <br />
    <div class="box">
      <b>moderation ignored log</b>
      <table class="mod_ignored">
        <tr>
          <th>&nbsp;</th>
          <th>&nbsp;</th>
          <th class="data">action</th>
          <th>target</th>
          <th>reason</th>
          <th>&nbsp;</th>
          <th>comment</th>
        </tr>
        %%mod_ignored%%
      </table>
    </div>
    <br />
    %%pagination%%
    <br />
  </body>
</html>
